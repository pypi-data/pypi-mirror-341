{
    "mcpServers": {
        "daytona-interpreter": {
            "command": "/Users/USER/.local/bin/uv",
            "args": [
                "--directory",
                "/Users/USER/dev/daytona-mcp-interpreter",
                "run",
                "src/daytona_mcp_interpreter/server.py"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1",
                "MCP_DAYTONA_API_KEY": "api_key",
                "MCP_DAYTONA_SERVER_URL": "https://app.daytona.io/api",
                "MCP_DAYTONA_TIMEOUT": "180.0",
                "MCP_VERIFY_SSL": "false",
                "MCP_DAYTONA_TARGET": "us",
                "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
            },
            "logFile": "~/Library/Logs/Claude/mcp-server-daytona-interpreter.log"
        }
    }
}