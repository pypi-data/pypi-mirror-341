    <script type="text/javascript">
        var rank_lists = document.querySelectorAll('.nav p');
        rank_lists.forEach(function(rank_lists) {
            rank_lists.addEventListener('click', function() {
                // 移除其他元素的激活状态
                rank_lists.forEach(function(item) {
                    item.setAttribute('data-active', 'false');
                });

                // 设置当前点击的元素为激活状态
                rank_lists.setAttribute('data-active', 'true');

                // 移动指示标到当前元素
                move_indicator(rank_lists);

                // 每次点击后重新设置最后一个导航项的 margin-right
                set_last_item_margin();
            });
        });
        var now_rank = rank_lists[{{rank_type}}]
    </script>
