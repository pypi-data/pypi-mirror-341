{
  "name": "@octostar/platform-types",
  "version": "0.5.10",
  "description": "Types for the API exposed by Octostar platform to third parties",
  "keywords": [
    "octostar",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/octostar-platform-types"
  },
  "license": "MIT",
  "author": "",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/index.d.ts",
    "dist/docs"
  ],
  "devDependencies": {
    "@elastic/elasticsearch": "^8.15.0",
    "@microsoft/api-extractor": "^7.42.3",
    "antd": "^5.10.3",
    "csstype": "^3.1.3",
    "flexlayout-react": "github:Octostarco/flexlayout-react#multi-window-drag-drop",
    "rc-menu": "^9.8.2",
    "rimraf": "^5.0.5",
    "typedoc": "^0.26.5",
    "typescript": "~5.7.2",
    "concurrently": "^8.0.0"
  },
  "peerDependencies": {
    "antd": "^5.10.3",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run compile && pnpm run run:api-extractor && pnpm run build:docs",
    "build:docs": "typedoc",
    "clean": "rimraf ./lib ./dist ./temp ./.turbo tsconfig.tsbuildinfo",
    "compile": "tsc",
    "run:api-extractor": "api-extractor run --local --verbose",
    "dev": "concurrently \"tsc --watch\" \"pnpm run compile && pnpm run run:api-extractor && typedoc --watch\""
  }
}